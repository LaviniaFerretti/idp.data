         <head xml:lang="en">
            <date>AD 266/7</date>
            <placeName>Hermoupolis Magna</placeName>
         </head>
         <div xml:lang="grc" type="edition" xml:space="preserve"><div n="r" type="textpart"><ab>
</ab></div><div n="Fr3" type="textpart"><ab>

    <lb n="1"/>τῇ κρατίστῃ βουλῇ Ἑρμουπόλεως 

    <lb n="2"/>τῆς μεγάλης ἀρχαίας καὶ λαμπρᾶς 

    <lb n="3"/>καὶ <supplied reason="lost">σε</supplied>μνωτάτης διʼ Αὐρηλίου Κο

    <lb n="4" break="no"/>ρελλίου Ἀ<supplied reason="lost">λ</supplied>εξάνδρου ἱππικοῦ ἀπὸ 

    <lb n="5"/>στρατιῶν γυμνασιάρχου βουλευ

    <lb n="6" break="no"/>τοῦ ἐνάρχου π<supplied reason="lost">ρ</supplied>υτάνεως τῆς 

    <lb n="7"/>πόλεως καὶ ὡς χρηματίζει 

    <lb n="8"/>παρὰ Αὐρηλίου Ἑρμ<supplied reason="lost">οῦ</supplied> Τατήριος 

    <lb n="9"/><unclear>Ἑ</unclear><supplied reason="lost">ρμοπολί</supplied>του ἀ<unclear>ν</unclear>αγραφομένου 

    <lb n="10"/>ἐπʼ ἀμφό<supplied reason="lost">δ</supplied>ου <expan>πόλ<ex>εως</ex></expan> <expan>ἀπηλ<ex>ιώτου</ex></expan> <choice><reg>βούλομαι</reg><orig>βούλομε</orig></choice> 

    <lb n="11"/><supplied reason="lost">ἑ</supplied>κου<supplied reason="lost">σίω</supplied>ς μισθώσασθαι ἐκ τοῦ πο

    <lb n="12" break="no"/><supplied reason="lost">λ</supplied><unclear>ι</unclear>τικοῦ λόγο<supplied reason="lost">υ</supplied> ἐπʼ ἔτη δύο <num value="2"/> ἀπὸ τοῦ 

    <lb n="13"/><supplied reason="lost">ὄν</supplied>τ<supplied reason="lost">ο</supplied>ς μ<unclear>ην</unclear>ὸς Ἁθὺρ τοῦ ἐνεστῶτος 

    <lb n="14"/><num value="14"><unclear>ι</unclear>δ</num> <expan><ex>ἔτους</ex></expan> οἰκίαν μ<supplied reason="lost">ο</supplied>νόστ<supplied reason="lost">εγ</supplied>ον καινὴν 

    <lb n="15"/>ἐν Ἑρμουπόλει ἐπὶ <expan>πόλ<ex>εως</ex></expan> ἀπηλιώτου 

    <lb n="16"/>πρὸς τῇ κα<unclear>μ</unclear>άρᾳ ὑπὸ στοὰν Ἀντι

    <lb n="17" break="no"/>νο<hi rend="diaeresis">ι</hi>τικῆς <choice><reg>πλατείας</reg><orig>πλατίας</orig></choice> εἰς οἴκησιν 

    <lb n="18"/>μοι καὶ τῶν παρʼ ἐμοῦ ἐνοι<supplied reason="lost">κίου</supplied> 

    <lb n="19"/>ἑκάστου μηνὸς δραχμῶν ὀκτώ <num value="8"/> 

    <lb n="20"/>ἃς ἀ<unclear>πο</unclear>δώ<supplied reason="lost">σ</supplied>ω καθʼ ἕκαστον 

    <lb n="21"/>μῆνα ἀνυπερθέτως καὶ ἐν τέ

    <lb n="22" break="no"/>λει τοῦ χρόνου παραδώσω σὺν 

    <lb n="23"/>θύραι<unclear>ς</unclear> <choice><reg>οὖσαν</reg><orig>ὀν</orig></choice> <supplied reason="lost">ὡ</supplied><unclear>ς</unclear> <choice><reg cert="low">παρείληφα </reg><orig>παρίληφα</orig></choice> ἐπιθέ

    <lb n="24" break="no"/>ματος δὲ <choice><reg>γενομένου</reg><orig>γεινομένου</orig></choice> ἐξὸν ἑτέ

    <lb n="25" break="no"/>ροις μεταμισθοῦν <choice><reg>ἐπερωτη<lb n="26" break="no"/>θεὶς</reg><orig>ἐπερωτη<lb n="26" break="no"/>θεισα</orig></choice> ὡμολόγ<supplied reason="lost">ησα.<expan><ex>ἔτους</ex></expan><num value="14">ιδ</num></supplied> 

    <lb n="27"/>Αὐτοκράτορος <supplied reason="lost">Καίσαρος</supplied> 

    <lb n="28"/>Πουπλίου Λικι<supplied reason="lost">ννίου Γαλλιηνοῦ</supplied> 

    <lb n="29"/>Γερμανικοῦ Μεγίστου <supplied reason="lost">Περσικοῦ</supplied> 

    <lb n="30"/>Μεγίστου Εὐσεβοῦς Εὐτ<supplied reason="lost">υχοῦς</supplied> 

    <lb n="31"/>Σεβ<supplied reason="lost">ασ</supplied>τοῦ Ἁθὺρ <num value="13">ιγ</num> <expan>Α<supplied reason="lost">ὐρ<ex>ήλιος</ex></supplied></expan> 

    <lb n="32"/>Ἑρμῆς Τατήριος <expan>μεμίσθ<ex>ωμαι</ex></expan> 

    <lb n="33"/>ὡς <expan>προκ<ex>εῖται</ex></expan> <expan>Α<ex>ὐρήλιος</ex></expan> Κοπρῆς <expan>ἔγρ<ex>αψα</ex></expan> <expan>ὑ<ex>πὲρ</ex></expan> <expan>αὐτ<ex>οῦ</ex></expan> 

    <lb n="34"/><expan>φάσκ<ex>οντος</ex></expan> μὴ <expan>εἰδ<ex>έναι</ex></expan> <expan>γρ<ex>άμματα</ex></expan> 
</ab></div></div>
