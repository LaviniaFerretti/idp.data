    <lb n="11"/><gap reason="lost" extent="unknown" unit="character"/>ταγμα ἀπειλ<gap reason="lost" quantity="10" unit="character"/> 

    <lb n="12"/><gap reason="lost" extent="unknown" unit="character"/> καὶ δεκάπρωτον ὡς ἐπιχειρης<gap reason="lost" extent="unknown" unit="character"/> 

