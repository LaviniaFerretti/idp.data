<div n="I" subtype="column" type="textpart" corresp="#FR10085"><ab> 
<lb n="1"/><gap reason="lost" extent="unknown" unit="line"/>
<lb n="1"/><g rend="diple" type="filler"/> <q><supplied reason="lost">πρὸς τὸ πλῆ</supplied><unclear>θος</unclear> <unclear>μὴ</unclear> <app type="alternative"><lem><unclear>προ</unclear><supplied reason="lost">σ</supplied></lem><rdg><unclear>πρ</unclear><supplied reason="lost">ο</supplied></rdg></app></q>
